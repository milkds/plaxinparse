    require([
        'jquery',
        'priceBox'
    ], function($){
        var dataPriceBoxSelector = '[data-role=priceBox]',
            dataProductIdSelector = '[data-product-id=6225]',
            priceBoxes = $(dataPriceBoxSelector + dataProductIdSelector);

